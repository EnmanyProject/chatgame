                {
                  "text_template": "\"귀엽다 ㅎㅎ 나도 기대돼\"",
                  "affection_range": "medium",
                  "tone": "playful"
                },
                {
                  "text_template": "\"우리 좋은 시간 보내자\"",
                  "affection_range": "medium",
                  "tone": "caring"
