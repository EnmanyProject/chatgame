        "act_title": "데이트 제안하기",
        "beats": [
          {
            "beat_number": 1,
            "beat_type": "introduction",
            "template": {
              "npc_dialogue_template": "${char_name}: 오빠 😊 이번 주말에 시간 있어?",
              "narration_template": "${char_name}(이)가 조심스럽게 주말 계획을 물어본다.",
              "choice_templates": [
                {
                  "text_template": "\"응! 왜? 뭐 계획 있어?\"",
                  "affection_range": "medium",
                  "tone": "gentle"
                },
                {
                  "text_template": "\"너랑 시간 보내고 싶은데 어때?\"",
                  "affection_range": "high",
                  "tone": "caring"
                },
                {
                  "text_template": "\"주말? 같이 뭐 할래?\"",
                  "affection_range": "low",
                  "tone": "playful"
                }
              ],
              "emotion_hint": "기대, 설렘, 조심스러움"
